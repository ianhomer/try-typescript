{
  "name": "try-typescript-node",
  "version": "0.0.1",
  "description": "Try Typescript with Node.js",
  "license": "BSD",
  "author": "",
  "keywords": [],
  "main": "index.js",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "format": "prettier --write 'src/**/*.{ts}'",
    "lint": "eslint --ext ts,tsx ./src",
    "lint:fix": "eslint --ext ts,tsx ./src --fix",
    "start": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "start:dist": "node ./dist/index.js",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/supertest": "^2.0.12",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/find-config": "^1.0.1",
    "@types/jest": "^29.5.0",
    "acorn": "^8.8.2",
    "dotenv": "^16.0.3",
    "find-config": "^1.0.0",
    "jest": "29.5.0",
    "minimist": "^1.2.8",
    "rimraf": "^4.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
